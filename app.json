{
  "expo": {
    "name": "ovdapp",
    "slug": "ovdproject",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/ovdIcon.png",
    "userInterfaceStyle": "light",
    "plugins": [
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "splash": {
      "image": "./assets/ovdSplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5D3FD3"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ovdapp.app",
      "buildNumber": "1.0.1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ovdIcon.png",
        "backgroundColor": "#0096FF"
      },
      "package": "com.taran_9125.ovdproject"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "eab30f67-a81b-4083-9a73-a098d7450731"
      }
    }
  }
}
